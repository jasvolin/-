a, b, c = int(input()), int(input()), input()
if c == "-":
    print (a - b)
elif c == "+":
    print(a + b)
elif c == "*":
    print(a * b)
elif c != "-" and c != "+" and c != "*" and c!= "/":
    print("Неверная операция")
elif b == 0:
    print("На ноль делить нельзя!")
else:
    print(a / b)
    
